# Ignore docker
/.env
/Makefile
/docker-compose.yml
/docker-sync.yml
/docker.mk
/load.environment.php
/mariadb-init/

# Ignore files generated by Drupal
/sites/*/files/*
/sites/*/private/*
/sites/default/settings.php

# Ignore Drupal core files (except .htaccess)
/core/*
!/core/.htaccess

# Ignore contributed modules and themes
/modules/contrib/*
/themes/contrib/*

# Ignore files generated by IDEs
*.idea
*.vscode

# Ignore vendor
/vendor
